{
  "name": "package-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.8.0",
  "description": "",
  "license": "MIT",
  "repository": "Teages/repo",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./components/*": {
      "import": {
        "types": "./dist/components/*/index.d.mts",
        "default": "./dist/components/*/index.mjs"
      },
      "require": {
        "types": "./dist/components/*/index.d.cts",
        "default": "./dist/components/*/index.cjs"
      }
    },
    "./utils/*": {
      "import": {
        "types": "./dist/utils/*/index.d.mts",
        "default": "./dist/utils/*/index.mjs"
      },
      "require": {
        "types": "./dist/utils/*/index.d.cts",
        "default": "./dist/utils/*/index.cjs"
      }
    },
    "./composables/*": {
      "import": {
        "types": "./dist/composables/*/index.d.mts",
        "default": "./dist/composables/*/index.mjs"
      },
      "require": {
        "types": "./dist/composables/*/index.d.cts",
        "default": "./dist/composables/*/index.cjs"
      }
    },
    "./styles/*.css": "./dist/styles/*.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=22.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev:prepare": "unbuild --stub",
    "dev": "pnpm -C playground dev",
    "dev:build": "pnpm -C playground build",
    "dev:preview": "pnpm -C playground preview",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release --push && pnpm publish",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@volar/typescript": "^2.4.10",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.5.7",
    "eslint": "^9.16.0",
    "fast-glob": "^3.3.2",
    "happy-dom": "^15.11.7",
    "jiti": "2.4.1",
    "mkdist": "npm:@teages/mkdist@1.6.0-patch.251.002c5b4",
    "sass": "^1.82.0",
    "typescript": "^5.7.2",
    "unbuild": "3.0.0-rc.11",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  },
  "resolutions": {
    "mkdist": "npm:@teages/mkdist@1.6.0-patch.251.002c5b4"
  },
  "publishConfig": {
    "access": "public"
  }
}
