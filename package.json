{
  "name": "package-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.8.0",
  "description": "",
  "license": "MIT",
  "repository": "Teages/repo",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.mjs",
      "require": "./dist/components/*/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.mjs",
      "require": "./dist/utils/*.js"
    },
    "./composables/*": {
      "types": "./dist/composables/*.d.ts",
      "import": "./dist/composables/*.mjs",
      "require": "./dist/composables/*.js"
    },
    "./styles/*.css": "./dist/styles/*.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev:prepare": "unbuild --stub",
    "dev": "pnpm -C playground dev",
    "dev:build": "pnpm -C playground build",
    "dev:preview": "pnpm -C playground preview",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release --push && pnpm publish",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "vue": "^3.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.4.1",
    "@teages/mkdist-vue-loader": "link:../mkdist-vue-loader",
    "@types/node": "^20",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-v8": "^2.0.5",
    "@volar/typescript": "^2.4.4",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.5.5",
    "eslint": "^9.10.0",
    "fast-glob": "^3.3.2",
    "happy-dom": "^15.7.3",
    "jiti": "2.0.0-beta.3",
    "mkdist": "1.5.6",
    "sass": "^1.78.0",
    "typescript": "^5.5.4",
    "unbuild": "^v3.0.0-rc.7",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
