{
  "name": "package-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.0",
  "description": "",
  "license": "MIT",
  "repository": "Teages/repo",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./components/*": {
      "types": "./dist/components/*.vue.d.ts",
      "default": "./dist/components/*.vue"
    },
    "./utils/*": {
      "import": {
        "types": "./dist/utils/*/index.d.mts",
        "default": "./dist/utils/*/index.mjs"
      },
      "require": {
        "types": "./dist/utils/*/index.d.cts",
        "default": "./dist/utils/*/index.cjs"
      }
    },
    "./composables/*": {
      "import": {
        "types": "./dist/composables/*/index.d.mts",
        "default": "./dist/composables/*/index.mjs"
      },
      "require": {
        "types": "./dist/composables/*/index.d.cts",
        "default": "./dist/composables/*/index.cjs"
      }
    },
    "./unplugin": {
      "import": {
        "types": "./dist/unplugin.d.mts",
        "default": "./dist/unplugin.mjs"
      },
      "require": {
        "types": "./dist/unplugin.d.cts",
        "default": "./dist/unplugin.cjs"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite.d.mts",
        "default": "./dist/vite.mjs"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    },
    "./nuxt": {
      "import": {
        "types": "./dist/nuxt.d.mts",
        "default": "./dist/nuxt.mjs"
      },
      "require": {
        "types": "./dist/nuxt.d.cts",
        "default": "./dist/nuxt.cjs"
      }
    },
    "./styles": "./dist/styles/index.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=22.0.0"
  },
  "scripts": {
    "dev": "pnpm -C playground dev",
    "dev:nuxt": "pnpm -C playground-nuxt dev:dist",
    "dev:dist": "pnpm -C playground dev:dist",
    "dev:dist:nuxt": "pnpm -C playground-nuxt dev:dist",
    "codegen": "jiti ./scripts/codegen.ts",
    "build": "pnpm codegen && unbuild",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release --push && pnpm publish",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix ."
  },
  "peerDependencies": {
    "vue": "^3.5"
  },
  "dependencies": {
    "@nuxt/kit": ">=3.13.1",
    "@nuxt/schema": ">=3.13.1",
    "defu": "^6.1.4",
    "local-pkg": "^0.5.1",
    "pathe": "^1.1.2",
    "unimport": "^3.14.5",
    "unplugin": "^2.1.0",
    "unplugin-vue-components": "^0.27.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@volar/typescript": "^2.4.11",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.5.7",
    "eslint": "^9.17.0",
    "happy-dom": "^15.11.7",
    "jiti": "2.4.1",
    "knitwork": "^1.1.0",
    "mlly": "^1.7.3",
    "sass": "^1.83.0",
    "scule": "^1.3.0",
    "tinyglobby": "^0.2.10",
    "typescript": "^5.7.2",
    "unbuild": "3.0.1",
    "vite": "^6.0.3",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
